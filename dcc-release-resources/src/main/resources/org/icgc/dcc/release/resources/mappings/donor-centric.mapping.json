{
  "donor-centric":{
    "_source":{
      "compress":"true",
      "excludes": ["gene.*"]
    },
    "_all":{
      "enabled":"false"
    },
    "dynamic_templates":[
      {
        "template_1":{
          "match":"*",
          "match_mapping_type":"string",
          "mapping":{
            "index":"not_analyzed"
          }
        }
      }
    ],
    "date_detection":"false",
    "properties":{
      "gene":{
        "type":"nested",
        "properties":{
          "ssm":{
            "type":"nested",
    	        "properties":{
    	          "consequence":{
    	            "type":"nested"
    	          },
                 "observation":{
                  "type":"nested"
                }
    	       }           
          }  
        }
      },
      "specimen":{
        "properties":{
          "sample":{
            "properties":{
            }
          }
        }
      },
      "project":{
        "properties":{
          "project_name":{
            "type":"string",
            "store":"yes",
            "index":"not_analyzed"
          }
        }
      }      
    }
  }
}
